  .vertical-center::before, .vertical-center > *{
    display: inline-block;
    vertical-align: middle;
  }
  .vertical-center::before {
    content: "";
    height: 100%;
  }
  .vertical-center {
    height: 100%;
    width: 100%;
  }
  .text-center{
    text-align: center;
  }
  .container {
    margin-left: auto;
    margin-right: auto;
    padding-left: 15px;
    padding-right: 15px;
    position: relative;
  }
  #page_wrapper{
    width: 100%;
    -webkit-transition: -webkit-transform 1s ease;
    -moz-transition: transform 1s ease;
    -o-transition: transform 1s ease;
    -ms-transition: transform 1s ease;
    transition: transform 1s ease;
    position: relative;
    z-index: 0;
  }
  #page_wrapper section{
    position: relative;
    margin: 0 auto;
    height: 100%;
    width: 100%;
    z-index: 0;
  }
  section{
    height: 100%;
    width: 100%;
    min-height: 100vh;
    position: relative;
    overflow: hidden;
    text-align: center;
    vertical-align: middle;
    margin: 0 auto;
    background-size: cover;
  }
  *::before, *::after {
    box-sizing: border-box;
  }
  #nav {
    right: 40px;
    list-style: outside none none;
    margin: -35px 0 0;
    padding: 0;
    position: a;
    top: 50%;
    z-index: 5;
    position: fixed;
    height: 70px;
  }
  #nav li {
    height: 20px;
    padding: 0;
    width: 14px;
  }
  #nav li a::before {
    background: #fff none repeat scroll 0 0;
    border: 3px solid #fff;
    border-radius: 20px;
    content: "";
    display: block;
    height: 8px;
    position: absolute;
    transition: all 0.5s ease 0s;
    width: 8px;
  }
  #nav li.active a::before {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    height: 14px;
    margin: -3px 0 0 -3px;
    width: 14px;
  }

  .fp-viewing-share #nav li a::before{
    background: #000 none repeat scroll 0 0;
    border-color: #000;
  }
  #nav li.active a::before {
    background: rgba(255, 244, 255, 0) none repeat scroll 0 0;
  }
  .nav > li > a:focus, .nav > li > a:hover{
    background-color: transparent !important;
    outline: none;
  }
/* Common Styles Ends **/

/** Phone wrapper style starts **/
#overlay{
  position: absolute;
  z-index: 4;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  pointer-events: none;
}
#overlay .container{
  position: relative;
  z-index: 9;
}
#overlay .container .row .pull-left{
  z-index: 9999;
}
#overlay .container .row .pull-right{
  z-index: 0;
}
#download{
  text-align: center;
  position: absolute;
  bottom: 40px;
}
#download .tech-info{
  /*background: url(<%= asset_path 'tech_info.png' %>) no-repeat center top;*/
  width: 198px;
  display: block;
  text-indent: -9999px;
  height: 58px;
  margin: 20px auto;
}
#phone_wrapper{
  background: url(<%= asset_path 'home/phone_wrapper.png' %>) no-repeat scroll center center / auto 90%;
  position: relative;
  width: 90%;
  height: 100%;
  z-index: 0 !important;
}
#phone_wrapper::after{
  content: " ";
  display: block;
  padding-top: 205.51%;
}
#phone_wrapper #phone_container {
  bottom: 16%;
  left: 10.94%;
  overflow: hidden;
  position: absolute;
  right: 10.4%;
  top: 15.8%;
}
#phone_wrapper #phone_container #images{
  height: 100%;
  position: absolute;
  width: 100%;
  transform: translateY(0px);
}
#phone_wrapper #phone_container #images img{
  height: auto;
  margin: 0;
  width: 100%;
  overflow-y: hidden;
  box-sizing: border-box;
}
#phone_wrapper #phone_container #images a{
  padding: 0;
  margin: 0;
}
/** Phone wrapper style ends **/

