<div class="container">
  <h2>Admin Details</h2>
  <div class="bs-docs-section">
    <div class="row">
      <div class="col-lg-12">
       <div>
         <strong>Name:</strong>
         <span><%= @admin.name %></span>
       </div>
       <div>
         <strong>Email:</strong>
         <span><%=@admin.email %></span>
       </div>
       <div>
         <strong>Created At:</strong>
         <span><%=@admin.created_at.strftime("%v") %></span>
       </div>
      </div>
    </div>
    <br>
    <%= link_to "Back", admin_admins_path, class: "btn btn-primary btn-sm" %>
  </div>
</div>
