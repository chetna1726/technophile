<div class="container">
  <div class="bs-docs-section">
    <div class="row">
      <div class="col-lg-12">
        <h2>Contact Requests</h2>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="bs-component">
            <% if @users.empty? %>
              <p class="lead text-info">No requests found!</p>
            <% else %>
              <table class="table table-striped table-hover ">
                <thead>
                  <tr class='success'>
                    <th>#</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Email</th>
                    <th>Email</th>
                    <th>Sent At</th>
                  </tr>
                </thead>
                  <%= render @users %>
              </table>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
